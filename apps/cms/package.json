{
  "name": "@acme/cms",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "yarn with-env next build",
    "dev": "yarn with-env next dev -p 3001",
    "devsafe": "rm -rf .next && with-env next dev",
    "generate:types": "with-env payload generate:types",
    "lint": "yarn with-env next lint",
    "payload": "yarn with-env payload",
    "start": "yarn with-env next start",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@acme/payload": "workspace:*",
    "@payloadcms/next": "beta",
    "@payloadcms/ui": "beta",
    "graphql": "^16.9.0",
    "next": "15.0.0-rc.0",
    "react": "^19.0.0-rc-6230622a1a-20240610",
    "react-dom": "^19.0.0-rc-6230622a1a-20240610"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:*",
    "@acme/prettier-config": "workspace:*",
    "@acme/tailwind-config": "workspace:*",
    "@acme/tsconfig": "workspace:*",
    "@types/node": "^20.14.14",
    "@types/react": "npm:types-react@19.0.0-rc.0",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.8.0",
    "typescript": "^5.5.4"
  },
  "overrides": {
    "@types/react": "npm:types-react@19.0.0-rc.0",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.0"
  },
  "prettier": "@acme/prettier-config"
}
