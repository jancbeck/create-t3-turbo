{
  "name": "create-t3-turbo",
  "private": true,
  "engines": {
    "node": ">=20.12.0"
  },
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "build": "turbo run build",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo run clean",
    "db:push": "turbo -F @acme/db push",
    "db:studio": "turbo -F @acme/db studio",
    "dev": "turbo watch dev",
    "dev:next": "turbo watch dev -F @acme/nextjs...",
    "format": "turbo run format --continue -- --cache --cache-location node_modules/.cache/.prettiercache",
    "format:fix": "turbo run format --continue -- --write --cache --cache-location node_modules/.cache/.prettiercache",
    "lint": "turbo run lint --continue -- --cache --cache-location node_modules/.cache/.eslintcache",
    "lint:fix": "turbo run lint --continue -- --fix --cache --cache-location node_modules/.cache/.eslintcache",
    "lint:ws": "yarn dlx sherif@latest",
    "postinstall": "yarn lint:ws",
    "typecheck": "turbo run typecheck",
    "ui-add": "turbo run ui-add"
  },
  "devDependencies": {
    "@acme/prettier-config": "workspace:*",
    "@turbo/gen": "^2.0.8",
    "eslint": "^9.6.0",
    "prettier": "^3.3.2",
    "turbo": "^2.0.8",
    "typescript": "^5.5.3"
  },
  "resolutions": {
    "apps/*/react": "18.3.1",
    "apps/*/react-dom": "18.3.1",
    "packages/*/react": "^19.0.0-rc-6230622a1a-20240610",
    "packages/*/react-dom": "^19.0.0-rc-6230622a1a-20240610",
    "tooling/*/next": "15.0.0-rc.0",
    "zod": "^3.23.8",
    "@tanstack/react-query": "^5.51.9",
    "@trpc/client": "^11.0.0-rc.466",
    "@trpc/react-query": "^11.0.0-rc.466",
    "@trpc/server": "^11.0.0-rc.466"
  },
  "prettier": "@acme/prettier-config",
  "version": "0.0.0"
}
