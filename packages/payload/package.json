{
  "name": "@acme/payload",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc --build --force",
    "clean": "rm -rf .turbo node_modules",
